{
  "ELIGIBLE": [
    {
      "COTISATIONS_ELIGIBLE" : {
        "label": "Recherche cotisations éligibles",
        "filters": [
          {
            "label": "Par date de paiement",
            "fields": [
              {
                "name": "dateFrom",
                "label": "DU",
                "type": "text"
              },
              {
                "name": "dateTo",
                "label": "AU",
                "type": "text"
              }
            ]
          },
          {
            "label": "Par numéro d'adhérent",
            "fields" : [
              {
                "name": "membershipNumber",
                "label": "Numéro d'adhérent",
                "type": "text"
              }
            ]
          }
        ],
        "export_file_pattern": "%s_Cotisations_eligibles_par_%s_%s",
        "columns" : [
          {
            "caption": "Environnement %s - %s",
            "detailsCaption": "Détail de l'assuré",
            "labels": [
              {
                "name": "membershipNumber",
                "translation": "Numéro adhérent"
              },
              {
                "name": "contributionPaymentDate",
                "translation": "Date de dernier paiement de la cotisation"
              },
              {
                "name": "contributionCallPeriod",
                "translation": "Période des cotisations en mois"
              },
              {
                "name": "contributionCallYear",
                "translation": "Période des cotisations en année"
              },
              {
                "name": "setting",
                "translation": "Paramétrage"
              },
              {
                "name": "settingDescription",
                "translation": "Description du paramétrage"
              },
              {
                "name": "beneficiaryName",
                "translation": "Nom"
              },
              {
                "name": "beneficiaryFirstname",
                "translation": "Prénom"
              },
              {
                "name": "beneficiaryBirthdate",
                "translation": "Date de naissance du bénéficiaire"
              },
              {
                "name": "socialSecurityNumber",
                "translation": "Numéro de Sécurité sociale"
              },
              {
                "name": "campaignDate",
                "translation": "Date de la campagne"
              },
              {
                "name": "clientName",
                "translation": "Nom du client"
              }
            ],
            "config" : [
              {
                "familyId": {
                  "sortable": true
                }
              },
              {
                "contributionPaymentDate": {
                  "sortable": true
                }
              },
              {
                "contributionCallPeriod": {
                  "sortable": true
                }
              },
              {
                "contributionCallYear": {
                  "sortable": true
                }
              },
              {
                "conservationTime": {
                  "sortable": true
                }
              },
              {
                "clientName" : {
                  "sortable": true
                }
              }
            ]
          }
        ]
      }
    },
    {
      "PRESTATIONS_SANTE_ELIGIBLE": {
        "label": "Recherche des prestations santé éligibles",
        "filters": [
          {
            "label": "Par typologie prestation",
            "fields": [
              {
                "name": "healthBenefitTypology",
                "label": "Typologie",
                "type": "select",
                "options" : [
                  "Audio",
                  "Hôpital public",
                  "Clinique",
                  "Frais d'obsèques",
                  "Cas général"
                ]
              }
            ]
          },
          {
            "label": "Par date de paiement sur une période",
            "fields" : [
              {
                "name": "dateFrom",
                "label": "DU",
                "type": "text"
              },
              {
                "name": "dateTo",
                "label": "AU",
                "type": "text"
              }
            ]
          },
          {
            "label": "Par numéro d'adhérent",
            "fields" : [
              {
                "name": "membershipNumber",
                "label": "Numéro adhérent",
                "type": "text"
              }
            ]
          },
          {
            "label": "Par identifiant prestation",
            "fields" : [
              {
                "name": "healthBenefitNumber",
                "label": "Identifiant prestation",
                "type": "text"
              }
            ]
          }
        ],
        "file_pattern": "%s_Prestations-sante-eligibles_par_%s_%s",
        "columns" : [

        ]
      }
    }
  ]
}